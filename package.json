{
  "name": "postwork-collab",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "db:up": "docker compose -f docker/docker-compose.yml up -d",
    "db:down": "docker compose -f docker/docker-compose.yml down",
    "prisma:migrate": "prisma migrate dev --schema=apps/api/prisma/schema.prisma",
    "prisma:generate": "prisma generate --schema=apps/api/prisma/schema.prisma",
    "seed": "ts-node apps/api/prisma/seed.ts",
    "api": "npm -w apps/api run dev",
    "web": "npm -w apps/web run dev",
    "dev": "npm run api & npm run web"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/MattGaarder/postwork-collab/issues"
  },
  "homepage": "https://github.com/MattGaarder/postwork-collab#readme",
  "devDependencies": {
    "prisma": "^6.17.1"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "dotenv": "^17.2.3"
  }
}
